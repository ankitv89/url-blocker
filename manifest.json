{
    "manifest_version": 2,
    "name": "Url Blocker",
    "version": "1.0.0",
    "description": "Url Blocker",
    "background": {
        "scripts": ["main.js"],
        "persistent": true
      },
    "permissions": [
        "*://*/*",
        "webRequest",
        "webRequestBlocking"
    ]
}